<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="1024"
    android:viewportHeight="1024">
    
    <!-- Aero Flow Main Body -->
    <path
        android:pathData="M 250 750 C 200 750, 150 700, 180 650 Q 300 400, 500 200 C 650 50, 850 50, 900 100 C 950 150, 900 350, 750 500 Q 550 700, 300 780 C 280 790, 260 760, 250 750 Z">
        <aapt:attr name="android:fillColor">
            <gradient 
                android:startX="100" 
                android:startY="100" 
                android:endX="900" 
                android:endY="900" 
                android:type="linear">
                <item android:offset="0.0" android:color="#FFFF8C00"/>
                <item android:offset="1.0" android:color="#FFFF2400"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- Speed Line Accent (Turbo Effect) -->
    <path
        android:pathData="M 350 680 Q 500 600, 700 450"
        android:strokeWidth="30"
        android:strokeColor="#66FFFFFF"
        android:strokeLineCap="round"/>
</vector>